# Local defaults for OCR in pipeline + database manager.
# Copy this file to `.env.local` in repo root.

DBZCCG_OCR_ENGINE=ollama-glm-ocr
DBZCCG_OCR_OLLAMA_MODEL=glm-ocr:latest
DBZCCG_OCR_OLLAMA_ENDPOINT=http://127.0.0.1:11434
DBZCCG_OCR_OLLAMA_TIMEOUT_MS=300000
DBZCCG_OCR_OLLAMA_ATTEMPTS=3
DBZCCG_OCR_OLLAMA_RETRY_DELAY_MS=3000
DBZCCG_OCR_OLLAMA_KEEP_ALIVE=10m
