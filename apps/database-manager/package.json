{
  "name": "@dbzccg/database-manager",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev:web": "vite --host 127.0.0.1 --port 5180",
    "dev:electron": "wait-on tcp:5180 && cross-env VITE_DEV_SERVER_URL=http://127.0.0.1:5180 electron --no-sandbox --disable-setuid-sandbox electron/main.cjs",
    "dev": "concurrently -k --success first -n WEB,ELECTRON -c cyan,green \"pnpm run dev:web\" \"pnpm run dev:electron\"",
    "build": "vite build",
    "preview": "vite preview --host 127.0.0.1 --port 4180",
    "start": "electron --no-sandbox --disable-setuid-sandbox electron/main.cjs",
    "typecheck": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.2.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^5.4.14",
    "wait-on": "^8.0.2"
  }
}
